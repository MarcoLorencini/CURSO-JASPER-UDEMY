	<queryString language="SQL">
		<![CDATA[select 
    a.aluno_id,
    a.nome,
    a.avatar,
    case a.sexo when 'M' then 'Masculino' when 'F' then 'Feminino' when 'X' then 'indefinido' end sexo,
    a.data_nascimento,
